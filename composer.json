{
    "name": "rmzamora/notification-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNotificationBundle rmzamora forked",
    "keywords": ["page", "sonata", "cms", "rmzamora forked"],
    "homepage": "https://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNotificationBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.4",
        "sonata-project/doctrine-extensions": "~1.0",
        "sonata-project/easy-extends-bundle": "~2.1",
        "rmzamora/core-bundle": "2.5.0.*",
        "rmzamora/datagrid-bundle": "2.5.0.*",
        "jms/serializer-bundle": "~1.1",
        "nelmio/api-doc-bundle": "~2.4",
        "friendsofsymfony/rest-bundle": "~1.1"
    },
    "suggest": {
        "rmzamora/doctrine-orm-admin-bundle": "2.5.0.*",
        "guzzle/guzzle" : "3.*",
        "liip/monitor-bundle": "~1.0",
        "videlalvaro/php-amqplib": "2.0.*"
    },
    "require-dev": {
        "rmzamora/doctrine-orm-admin-bundle": "2.5.0.*",
        "guzzle/guzzle" : "~3.0",
        "videlalvaro/php-amqplib": "~2.0",
        "liip/monitor-bundle": "~2.0",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "fabpot/php-cs-fixer": "~0.5|~1.0",
        "swiftmailer/swiftmailer": "~4.3|~5"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\NotificationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.5.0": "2.5.0.x-dev"
        }
    }
}
